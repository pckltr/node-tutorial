<%- contentFor('hero'); %>
<section class="hero">
	<h2>About us</h2>
	<p>We are good people.</p>

	<a href="/contact" class="btn btn-lg btn-warning">Contact Us!</a>
</section>

<%- contentFor('body') %>
<div class="jumbotron text-center">
	<h1>Our People</h1>
	<div class="row">
		<% for (user of users) { %>
			<div class="col-sm-3">
				<h2><%= user.name %></h2>
				<p><%= user.email %></p>
			</div>
		<% } %>
	</div>
</div>
